{
  "name": "jest-mock-knex",
  "version": "0.0.0-development",
  "description": "Jest mock for knex",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "test": "eslint ./src; jest --coverage --forceExit ./src",
    "build": "rm -rf lib && babel src --ignore test.js --out-dir lib && babel src/serializer.js -o serializer.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yutin1987/jest-mock-knex.git"
  },
  "author": "YuTin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yutin1987/jest-mock-knex/issues"
  },
  "homepage": "https://github.com/yutin1987/jest-mock-knex#readme",
  "files": [
    "lib",
    "serializer.js"
  ],
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react-native": "^1.9.1",
    "bluebird": "^3.5.0",
    "bookshelf": "^0.10.3",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "faker": "^4.1.0",
    "jest": "^21.2.1",
    "knex": "^0.14.0",
    "pg": "^7.1.0",
    "semantic-release": "^6.3.6",
    "sqlite3": "^3.1.8"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "jest": {
    "clearMocks": true,
    "snapshotSerializers": [
      "<rootDir>/src/serializer"
    ]
  }
}
